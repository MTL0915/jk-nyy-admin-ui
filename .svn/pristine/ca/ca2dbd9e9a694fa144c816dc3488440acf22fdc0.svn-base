export function getLogTypeDes(type) {
    if (type =="BindDevice"){
        return '设备绑定'
    }else if (type == "UnBindDevice"){
        return "设备解绑"
    }else if (type == "CloseChannel" || type == "CloseChannels"){
        return "关闭"
    }else if (type == "OpenChannel" || type == "OpenChannels"){
        return "打开"
    }else if (type == "Sta" || type == "GetSta"){
        return "更新状态"
    }else if (type == "Online"){
        return "设备上线"
    }else if (type == "Offline"){
        return "设备离线"
    }else if (type == "SetPara"){
        return "设置"
    }else if (type == "CloseWindow"){
        return "关闭"
    }else if (type == "OpenWindow"){
        return "打开"
    }else if (type == "StopWindow"){
        return "停止"
    }else if (type == 'SensorFault') {
        return '传感器数值异常'
    } else if (type == 'SensorException') {
        return '传感器数值预警'
    }else {
        return '其它'
    }
  }
  

  export function getLogTypeColor(type){
    if (type =="BindDevice"){
        return '#00bcd4'
    }else if (type == "UnBindDevice"){
        return "red"
    }else if (type == "CloseChannel" || type == "CloseChannels"){
        return "red"
    }else if (type == "OpenChannel" || type == "OpenChannels"){
        return "#17cc67"
    }else if (type == "Sta" || type == "GetSta"){
        return "#00bcd4"
    }else if (type == "Online"){
        return "#00bcd4"
    }else if (type == "Offline"){
        return "red"
    }else if (type == "SetPara"){
        return "#17cc67"
    }else if (type == "CloseWindow"){
        return "#17cc67"
    }else if (type == "OpenWindow"){
        return "#17cc67"
    }else if (type == "StopWindow"){
        return "#17cc67"
    }else if (type == "SensorFault"){
        return "red"
    }else if (type == "SensorException"){
        return "red"
    }
  }

  
  export function getLogTypeIcon(type){
    if (type =="BindDevice"){
        return 'el-icon-s-data'
    }else if (type == "UnBindDevice"){
        return "el-icon-warning"
    }else if (type == "CloseChannel" || type == "CloseChannels"){
        return "el-icon-switch-button"
    }else if (type == "OpenChannel" || type == "OpenChannels"){
        return "el-icon-switch-button"
    }else if (type == "Sta" || type == "GetSta"){
        return "el-icon-s-data"
    }else if (type == "Online"){
        return "el-icon-s-data"
    }else if (type == "Offline"){
        return "el-icon-warning"
    }else if (type == "SetPara"){
        return "el-icon-setting"
    }else if (type == "CloseWindow"){
        return "el-icon-switch-button"
    }else if (type == "OpenWindow"){
        return "el-icon-switch-button"
    }else if (type == "StopWindow"){
        return "el-icon-switch-button"
    }else if (type == "SensorFault"){
        return "el-icon-warning"
    }else if (type == "SensorException"){
        return "el-icon-warning"
    }
  }