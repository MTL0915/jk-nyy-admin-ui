import request from '@/utils/request'

//精讯云设备列表
export function list (data) {
    return request({
      url: '/jingxun/list',
      method: 'get',
      params: data
    })
  }

  
//将精讯云设备添加到出厂设备
export function addJingxunDeviceFactory (data) {
    return request({
      url: '/jingxun/addJingxunDeviceFactory',
      method: 'post',
      params: data
    })
  }