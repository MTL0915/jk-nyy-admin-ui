import request from '@/utils/request'

export function add(data) {
  return request({
    url: 'question/questions',
    method: 'post',
    data
  })
}

export function del(id) {
  return request({
    url: 'question/questions/' + id,
    method: 'delete'
  })
}

export function edit(data) {
  return request({
    url: 'question/questions',
    method: 'put',
    data
  })
}

export function find(data) {
  return request({
    url: 'question/questions',
    method: 'get',
    params: data
  })
}

