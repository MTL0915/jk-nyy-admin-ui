import request from '@/utils/request'

//获取原始数据
export function recognitionDetails(data) {
    return request({
      url: 'recognition/recognitionDetails',
      method: 'get',
      params: data
    })
  }

//获取聚合查询数据
  export function recognitionGroup(data) {
    return request({
      url: 'recognition/recognitionGroup',
      method: 'get',
      params: data
    })
  }

  //获取设备最新一条识别数据
  export function getNewestData(data) {
    return request({
      url: 'recognition/getNewestData',
      method: 'get',
      params: data
    })
  }
  